<project>
    <masterversions>
        <package name="osx_config"                      version="1.20.01"/>
        <package name="ios_config"                      version="1.14.00"/>
        <package name="XcodeProjectizer"                version="3.07.02" />
        <package name="EABase"                          version="2.09.08"/>
        <package name="EAThread"                        version="1.32.17"/>
    </masterversions>

    <buildroot>build</buildroot>
    <ondemand>true</ondemand>
