      osx-deployment-target-version=10.12
      iphone-extra-frameworks="CFNetwork Foundation Security SystemConfiguration"
      osx-extra-frameworks="-framework Security"
    </globalproperties>

    <config package="eaconfig" default="iphonesim-x64-clang-dev-debug" includes="iphonesim-x64-clang-dev-* osx-x64-clang-dev*"/>   
</project>
